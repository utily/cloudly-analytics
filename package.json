{
	"name": "cloudly-analytics",
	"version": "0.1.0",
	"description": "Library to create analytics api.",
	"author": "Utily Contributors",
	"license": "MIT",
	"repository": "https://github.com/utily/cloudly-analytics",
	"bugs": {
		"url": "https://github.com/utily/cloudly-analytics/issues"
	},
	"homepage": "https://github.com/utily",
	"private": false,
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"type": "module",
	"git": {
		"tagName": "v${version}"
	},
	"github": {
		"release": true
	},
	"jest": {
		"globals": {
			"ts-jest": {
				"tsconfig": "tsconfig.test.json"
			}
		},
		"transform": {
			"^.+\\.(j|t)sx?$": "ts-jest"
		},
		"transformIgnorePatterns": [
			"<rootDir>/node_modules/(?!(cryptly|authly|isoly|gracely|cloudly-http|cloudly-router|cloudly-storage|cloudly-analytics)/.*)"
		],
		"testEnvironment": "node",
		"testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
		"testPathIgnorePatterns": [
			"node_modules/",
			"dist/",
			"test-worker/"
		],
		"collectCoverageFrom": [
			"**/*.{ts,tsx,js,jsx}",
			"!**/node_modules/**",
			"!**/dist/**"
		],
		"preset": "ts-jest",
		"testMatch": null
	},
	"scripts": {
		"lint": "eslint --max-warnings=0 '**/*.{ts,tsx}'",
		"fix": "eslint '**/*.{ts,tsx}' --fix",
		"build": "tsc -p .",
		"jest": "jest",
		"test": "cd test-worker/ && npm run test && cd .. && jest",
		"prepare": "npm run build",
		"clean": "rimraf dist node_modules coverage",
		"start": "npm --prefix test-worker run start",
		"uninstall": "rm -rf dist && rm -rf test-worker/dist"
	},
	"devDependencies": {
		"@types/jest": "^29.4.0",
		"@typescript-eslint/eslint-plugin": "5.54.0",
		"@typescript-eslint/parser": "5.54.0",
		"eslint": "^8.35.0",
		"eslint-plugin-prettierx": "github:utily/eslint-plugin-prettierx#utily-20221229",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"jest": "^29.4.3",
		"prettierx": "github:utily/prettierx#utily-20221229",
		"rimraf": "^4.1.2",
		"ts-jest": "^29.0.5",
		"typescript": "^4.9.5",
		"@cloudflare/workers-types": "^4.20230221.0"
	},
	"dependencies": {
		"cloudly-http": "github:utily/cloudly-http#tvartom-cloudflare-properties",
		"isly": "github:utily/isly#version-0-1"
	},
	"overrides": {
		"json5": "^2.2.2"
	},
	"overridesComments": {
		"json5": "TODO: Remove override if `npm i` gives no vulnerabilities for 2.0.0 - 2.2.1, Severity: high, Prototype Pollution in JSON5 via Parse Method - https://github.com/advisories/GHSA-9c47-m6qq-7p4h"
	}
}
